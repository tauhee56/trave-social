{
  "expo": {
    "name": "trave-social",
    "slug": "trave-social",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "travesocial",
    "userInterfaceStyle": "automatic",
    "extra": {
      "eas": {
        "projectId": "4d97a183-7794-47bd-b83a-ada113f325ea"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.tauhee56.travesocial",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.INTERNET"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCYpwO1yUux1cHtd2bs-huu1hNKv1kC18c"
        }
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tauhee56.travesocial"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "./plugins/withCleartextTraffic.js",
      "./plugins/withNetworkSecurityConfig.js",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera for posting photos and live streaming.",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone for live streaming.",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos for posting.",
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera for taking photos."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location for map features and location tagging.",
          "locationAlwaysPermission": "Allow $(PRODUCT_NAME) to use your location for map features.",
          "locationWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location while using the app.",
          "isAndroidBackgroundLocationEnabled": false,
          "isAndroidForegroundServiceEnabled": false
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "edc36fa0-5de0-465f-98c4-2f87cb8278d3"
      }
    },
    "owner": "tauheeddev"
  }
}
