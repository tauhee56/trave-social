# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Firebase Configuration
EXPO_PUBLIC_FIREBASE_API_KEY=your-firebase-api-key
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=your-project-id
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
EXPO_PUBLIC_FIREBASE_APP_ID=your-app-id
EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID=your-measurement-id

# Google Maps Configuration
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Agora Configuration (Live Streaming)
EXPO_PUBLIC_AGORA_APP_ID=your-agora-app-id
EXPO_PUBLIC_AGORA_APP_CERTIFICATE=your-agora-certificate
EXPO_PUBLIC_AGORA_TOKEN_URL=your-token-server-url

# App Configuration
NODE_ENV=development

# Optional: Analytics, Push Notifications, etc.
# EXPO_PUBLIC_MIXPANEL_TOKEN=your-mixpanel-token
# EXPO_PUBLIC_ONESIGNAL_APP_ID=your-onesignal-app-id
